{
  "name": "reto-3-auth",
  "version": "1.0.0",
  "description": "Servicio de autenticaci√≥n JWT con Hono",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "test:acceptance": "npx cucumber-js --require \"test/acceptance/steps/**/*.js\" --require \"test/acceptance/support/**/*.js\" --config cucumber.js \"test/acceptance/features/*.feature\"",
    "test:acceptance:report": "npx cucumber-js --require \"test/acceptance/steps/**/*.js\" --require \"test/acceptance/support/**/*.js\" \"test/acceptance/features/*.feature\" --format html:test/acceptance/reports/cucumber-report.html",
    "test:acceptance:watch": "nodemon --exec \"npx cucumber-js --require \\\"test/acceptance/steps/**/*.js\\\" --require \\\"test/acceptance/support/**/*.js\\\" \\\"test/acceptance/features/*.feature\\\"\"",
    "test:acceptance:headless": "npx cucumber-js --require \"test/acceptance/steps/**/*.js\" --require \"test/acceptance/support/**/*.js\" \"test/acceptance/features/*.feature\" --format json:test/acceptance/reports/cucumber-report.json --format progress-bar"
  },
  "type": "module",
  "dependencies": {
    "@hono/node-server": "^1.19.0",
    "@hono/swagger-ui": "^0.5.2",
    "@hono/zod-openapi": "^1.1.0",
    "amqplib": "^0.10.3",
    "bcryptjs": "^2.4.3",
    "body-parser": "^2.2.0",
    "consul": "^1.2.0",
    "dotenv": "^17.2.2",
    "hono": "^4.9.4",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.0",
    "swagger-ui-dist": "^5.28.1",
    "yaml": "^2.8.1",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^9.5.1",
    "ajv": "^8.12.0",
    "assert": "^2.1.0",
    "nodemon": "^3.0.1"
  }
}
